<odoo>
    <record id="document_logging.view_document_logging_report_wizard_form" model="ir.ui.view">
        <field name="name">document_logging.report_wizard_form</field>
        <field name="model">document_logging.report_wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="start_date" />
                    <field name="end_date" />
                </group>
                <footer>
                    <button string="Generate Report" type="object" class="oe_highlight"
                        name="generate_report" />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>